<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}"
    android:background="@drawable/ultimaayuda" >

    <Button
        android:id="@+id/btAtrasUltima"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="26dp"
        android:layout_marginLeft="17dp"
        android:background="@drawable/botonatras"
        android:onClick="atrasAyuda"/>

    <Button
        android:id="@+id/btVuelveMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btAtrasUltima"
        android:layout_alignBottom="@+id/btAtrasUltima"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24dp"
        android:background="@drawable/botonalante" 
        android:onClick="masAyuda"/>

</RelativeLayout>
